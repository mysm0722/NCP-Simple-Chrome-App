{
    "manifest_version": 2,
    "background": {
        "scripts": [ "background.js" ]
     },
    "name": "NCPChromeApp",
    "description": "Naver Cloud Platform Console",
    "version": "1.0",
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab",
        "webRequest", 
        "webRequestBlocking", 
        "\u003Call_urls>"
      ]
  }
